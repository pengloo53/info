        <div style="margin-bottom:20px">
            <h3>操作记录<small> - 用户：<%= username %> </small></h3>
            <!-- <a class="btn btn-danger btn-sm pull-right" style="margin-bottom: 20px" href="/user/fom/edit">编辑</a> -->
        </div>
        <div class="row">
            <div class="col-lg-12">
                <table class="table table-striped">
                    <tr>
                        <!-- <th>用户</th> -->
                        <th>操作</th>
                        <th>姓名</th>
                        <th>旧值</th>
                        <th>新值</th>
                        <th>IP地址</th>
                        <th>时间</th>
                    </tr>
                    <% for(var i =0; i < log.length; i++){ %>
                    <tr>
                        <!-- <td><%= log[i].username %></td> -->
                        <td><%= log[i].action %></td>
                        <td><%= log[i].name %></td>
                        <td><%= log[i].oldData %></td>
                        <td><%= log[i].newData %></td>
                        <td><%= log[i].ip %></td>
                        <td><%= log[i].createdAt %></td>
                    </tr>
                    <% } %>
                </table>
            </div>
        </div>
        <!-- </div>   -->
    </div>    
